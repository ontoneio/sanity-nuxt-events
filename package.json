{
  "private": true,
  "name": "sanity-nuxt-events",
  "version": "1.0.7",
  "engines": {
    "yarn": "latest"
  },
  "scripts": {
    "build": "lerna run build --parallel",
    "postbuild": "yarn workspace @sanity-nuxt-events-web@1.0.8 add @sanity-nuxt-events-studio@1.0.5",
    "dev": "lerna run dev --parallel",
    "format": "lerna run format",
    "build-studio": "cd studio && npm run build",
    "build-web": "(cd studio && SANITY_AUTH_TOKEN=$SANITY_DEPLOY_STUDIO_TOKEN npm run graphql-deploy) && (cd web && npm run build)",
    "graphql-deploy": "lerna run graphql-deploy",
    "lint": "lerna run lint",
    "postinstall": "lerna bootstrap",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "workspaces": [
    "web",
    "studio"
  ],
  "devDependencies": {
    "@sanity/cli": "^1.149.19",
    "lerna": "latest"
  }
}
